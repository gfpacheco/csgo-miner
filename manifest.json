{
  "name": "Skin Miner",
  "description": "Mine CS:GO skins (and free money) through websites",
  "version": "0.1",
  "manifest_version": 2,
  "icons": {
    "16": "icons/icon_16.png",
    "128": "icons/icon_128.png"
  },
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "permissions": [
    "webview",
    "notifications",
    "storage"
  ]
}
